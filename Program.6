/*Given an array A of sorted integers and another non negative integer k, find if there exists 2 indices i and j such that A[j] - A[i] = k, i != j*/
#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int k;
    scanf("%d",&k);
    int i=0;
    int j=1;
    while(j<n){
       int a=arr[j]-arr[i];
        if(i!=j && k==a){
            printf("1");
            return 0;
        }else if(a<k){
            j++;
        }else{
            i++;
        }
    }
    printf("0");
}
